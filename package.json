{
  "name": "@ciensprog/fnapi",
  "version": "1.1.0",
  "description": "Utilities for making requests to the Fortnite API and data formatters",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/Ciensprog/fnapi",
  "bugs": {
    "url": "https://github.com/Ciensprog/fnapi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ciensprog/fnapi.git"
  },
  "author": {
    "name": "Ciensprog"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./parsers/stw": {
      "import": "./dist/parsers/stw/index.mjs",
      "require": "./dist/parsers/stw/index.cjs"
    },
    "./services": {
      "import": "./dist/services/index.mjs",
      "require": "./dist/services/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "packageManager": "pnpm@10.25.0+sha512.5e82639027af37cf832061bcc6d639c219634488e0f2baebe785028a793de7b525ffcd3f7ff574f5e9860654e098fe852ba8ac5dd5cefe1767d23a020a92f501",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "12.0.2",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@types/node": "24.10.1",
    "bumpp": "10.3.1",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "semantic-release": "25.0.2",
    "tsdown": "0.16.4",
    "typescript": "5.9.3",
    "vitest": "4.0.9"
  }
}
